{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "pretypeorm": "ts-node -r tsconfig-paths/register ./src/database/scripts/copy-typeorm-config.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seeds": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js",
    "migration:generate": "node -e \"const name = process.env.npm_config_name || 'migration'; const command = 'npm run typeorm -- -d src/database/data-source-dev.ts migration:generate src/database/migrations_dev/' + name; require('child_process').execSync(command, { stdio: 'inherit', shell: process.platform === 'win32' });\"",
    "migration:prod:generate": "node -e \"const name = process.env.npm_config_name || 'migration'; const command = 'npm run typeorm -- -d src/database/data-source-prod.ts migration:generate src/database/migrations/' + name; require('child_process').execSync(command, { stdio: 'inherit', shell: process.platform === 'win32' });\"",
    "migration:hml:generate": "node -e \"const name = process.env.npm_config_name || 'migration'; const command = 'npm run typeorm -- -d src/database/data-source-hml.ts migration:generate src/database/migrations_hml/' + name; require('child_process').execSync(command, { stdio: 'inherit', shell: process.platform === 'win32' });\"",
    "migration:create": "node -e \"const name = process.env.npm_config_name || 'migration'; const command = 'npm run typeorm -- migration:create src/database/migrations_dev/' + name; require('child_process').execSync(command, { stdio: 'inherit', shell: process.platform === 'win32' });\"",
    "migration:prod:create": "node -e \"const name = process.env.npm_config_name || 'migration'; const command = 'npm run typeorm -- migration:create src/database/migrations/' + name; require('child_process').execSync(command, { stdio: 'inherit', shell: process.platform === 'win32' });\"",
    "migration:hml:create": "node -e \"const name = process.env.npm_config_name || 'migration'; const command = 'npm run typeorm -- migration:create src/database/migrations_hml/' + name; require('child_process').execSync(command, { stdio: 'inherit', shell: process.platform === 'win32' });\"",
    "migration:run": "npm run typeorm migration:run -- --dataSource src/database/data-source-dev.ts",
    "migration:prod:run": "npm run typeorm migration:run -- --dataSource src/database/data-source-prod.ts",
    "migration:hml:run": "npm run typeorm migration:run -- --dataSource src/database/data-source-hml.ts",
    "migration:revert": "npm run typeorm -- -d src/database/data-source-dev.ts migration:revert",
    "migration:prod:revert": "npm run typeorm -- -d src/database/data-source-prod.ts migration:revert",
    "migration:hml:revert": "npm run typeorm -- -d src/database/data-source-hml.ts migration:revert",
    "migration:dev:revert": "npm run typeorm -- -d src/database/data-source-dev.ts migration:revert",
    "schema:drop": "npm run typeorm -- schema:drop",
    "seed:config": "npm run seeds -- config",
    "seed:run": "npm run seeds -- seed",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint src --ext ts --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json --detectOpenHandles --force-exit",
    "deploy": "sls deploy --verbose && serverless deploy --stage production --verbose"
  },
  "dependencies": {
    "@bull-board/api": "^6.0.0",
    "@bull-board/express": "^6.0.0",
    "@bull-board/nestjs": "^6.0.0",
    "@google-cloud/bigquery": "^7.9.1",
    "@google-cloud/storage": "^7.13.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.0.0",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@react-pdf/renderer": "^3.4.4",
    "@sentry/cli": "^2.36.4",
    "@sentry/nestjs": "^8.32.0",
    "@sentry/profiling-node": "^8.32.0",
    "@types/aws-lambda": "^8.10.131",
    "@types/bcryptjs": "2.4.6",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.4",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "@vendia/serverless-express": "^4.12.6",
    "adm-zip": "^0.5.12",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "2.1533.0",
    "bcryptjs": "2.4.3",
    "bullmq": "^5.14.0",
    "cache-manager": "^5.5.2",
    "chardet": "^2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.1",
    "crypto-ts": "^1.0.2",
    "date-holidays": "^3.23.3",
    "dayjs": "^1.11.10",
    "env-cmd": "10.1.0",
    "eslint": "8.56.0",
    "express": "^4.19.2",
    "express-basic-auth": "^1.2.1",
    "handlebars": "^4.7.8",
    "husky": "8.0.3",
    "iconv-lite": "^0.6.3",
    "is-ci": "3.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "numero-por-extenso": "^1.0.8",
    "papaparse": "^5.4.1",
    "passport": "^0.7.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.1",
    "passport-local": "^1.0.0",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.15.0",
    "pg": "^8.13.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "request": "^2.88.2",
    "rxjs": "7.8.1",
    "serverless": "^3.38.0",
    "ts-loader": "9.5.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.2",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5"
  },
  "resolutions": {
    "chokidar": "2.1.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!src/database"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "src/database"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "test-config",
      "interfaces",
      "jestGlobalMocks.ts",
      ".module.ts",
      ".entity.ts",
      "<rootDir>/src/database",
      "database",
      ".mock.ts"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@types/adm-zip": "^0.5.5",
    "@types/jest": "^29.5.11",
    "@types/nodemailer": "^6.4.14",
    "@types/papaparse": "^5.3.14",
    "@types/passport-local": "^1.0.38",
    "@types/pdfkit": "^0.13.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "axios-mock-adapter": "^2.0.0",
    "clinic": "^13.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.26.10",
    "typedoc-plugin-merge-modules": "^6.0.3"
  }
}
