{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "is-ci || husky install",
    "pretypeorm": "ts-node -r tsconfig-paths/register ./src/database/scripts/copy-typeorm-config.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seeds": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migration:create": "npm run typeorm -- migration:create -n",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "schema:drop": "npm run typeorm -- schema:drop",
    "seed:config": "npm run seeds -- config",
    "seed:run": "npm run seeds -- seed",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint src --ext ts --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json --detectOpenHandles --force-exit",
    "deploy": "sls deploy --verbose && serverless deploy --stage production --verbose"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.3.1",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/cli": "8.0.0",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.1.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "8.2.2",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/testing": "8.4.7",
    "@nestjs/typeorm": "8.0.3",
    "@types/aws-lambda": "^8.10.101",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.13",
    "@types/facebook-js-sdk": "3.3.5",
    "@types/multer": "1.4.7",
    "@types/node": "16.11.41",
    "@types/passport-anonymous": "1.0.3",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "@types/twitter": "1.7.1",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "@vendia/serverless-express": "^4.5.4",
    "apple-signin-auth": "1.5.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "2.1137.0",
    "axios-mock-adapter": "^1.21.5",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "crypto": "^1.0.1",
    "crypto-ts": "^1.0.2",
    "date-holidays": "^3.21.0",
    "dayjs": "^1.11.7",
    "env-cmd": "10.1.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "fast-csv": "^4.3.6",
    "fb": "2.0.0",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.7",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "multer": "1.4.4",
    "multer-s3": "2.10.0",
    "node-fetch": "^3.2.10",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.7.6",
    "papaparse": "^5.4.1",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.0",
    "pdfkit": "^0.13.0",
    "pg": "8.7.3",
    "plugin": "^0.3.3",
    "prettier": "2.6.2",
    "qs": "^6.11.0",
    "querystring": "^0.2.1",
    "reflect-metadata": "0.1.13",
    "request": "^2.88.2",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "serverless": "^3.27.0",
    "serverless-offline": "8.5.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.2",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "swagger-ui-express": "4.3.0",
    "ts-loader": "9.3.1",
    "ts-node": "10.7.0",
    "tsconfig-paths": "4.0.0",
    "tslib": "2.4.0",
    "twitter": "1.7.1",
    "typeorm": "0.2.43",
    "typeorm-seeding": "1.6.1",
    "typescript": "4.6.4",
    "url": "^0.11.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "resolutions": {
    "chokidar": "2.1.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!src/database"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "src/database"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "test-config",
      "interfaces",
      "jestGlobalMocks.ts",
      ".module.ts",
      ".entity.ts",
      "<rootDir>/src/database",
      "database",
      ".mock.ts"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/jest": "^29.5.3",
    "@types/nodemailer": "^6.4.9",
    "@types/papaparse": "^5.3.7",
    "@types/pdfkit": "^0.12.10",
    "@types/uuid": "^9.0.1",
    "axios-mock-adapter": "^1.21.5",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1"
  }
}
